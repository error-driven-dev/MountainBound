@model IEnumerable<string>

<h3>Categories</h3>
<Hr />
@foreach (string category in Model)
{
    <p><a class="btn btn-block @(ViewContext.RouteData.Values["category"]?.ToString() == category ? "tp-btn":"tp-btn-outline")" asp-action="ProductDirectory" asp-controller="Product" asp-route-category="@category" asp-route-pagenum="1">
        @category</a></p>

}

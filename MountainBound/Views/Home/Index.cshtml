@{ Layout = null;}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link asp-href-include="/lib/bootstrap/dist/css/*.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Mogra" rel="stylesheet">
    <link href="~/CSS/NavStyles.css" rel="stylesheet" />
    <link href="~/CSS/HomeStyles.css" rel="stylesheet" />


</head>
<body >

<div>@await Html.PartialAsync("../Shared/MasterNav.cshtml")</div>

<div class="landing">
    <div class="c-wrap carousel slide" id="carouselExampleControls" data-ride="carousel">

            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-75" src="~/images/landing.jpg" alt="First slide">
 
                </div>
                <div class="carousel-item">
                    <img class="d-block w-75 " src="~/images/gear_sale.jpg" alt="Second slide">

                </div>

                <div class="carousel-item">
                    <img class="d-block w-75" src="/images/trailhead.jpg" alt="Third slide">
   
                </div>
                <div class="carousel-item">
                    <img class="d-block w-75" src="~/images/campfire1.jpg" alt="Fourth slide">

                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>

</div>
<div>@await Html.PartialAsync("../Shared/MasterFooter.cshtml")</div>

<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>